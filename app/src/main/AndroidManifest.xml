<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.damindu.fish_project"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_hover"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Viewer"
            android:label="@string/title_activity_viewer" />
        <activity android:name=".Prohibited_button" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".Prob_content" />
        <activity android:name=".Restri_content" />
        <activity android:name=".Restricted_button" />




        <activity android:name=".Conf_content" />
        <activity android:name=".Conflicted_button" />
        <activity android:name=".About" />
        <activity android:name=".Acknowledge" />
        <activity android:name=".Statistics" />
    </application>

</manifest>